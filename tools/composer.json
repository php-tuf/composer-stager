{
    "name": "php-tuf/composer-stager-dev",
    "description": "Provides additional quality tools for Composer Stager development.",
    "license": "MIT",
    "type": "library",
    "require": {
        "wikimedia/composer-merge-plugin": "dev-master"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "wikimedia/composer-merge-plugin": true
        },
        "vendor-dir": "../vendor"
    },
    "extra": {
        "merge-plugin": {
            "include": [
                "../composer.json"
            ]
        }
    }
}
